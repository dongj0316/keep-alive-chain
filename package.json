    "build-lib": "vue-cli-service build --target lib --name v-keep-alive-chain --dest dist keep-alive-chain/index.js"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "v-keep-alive-chain": "^0.1.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0-beta.0",
    "@vue/cli-plugin-eslint": "^4.1.0-beta.0",
    "@vue/cli-plugin-router": "^4.1.0-beta.0",
    "@vue/cli-plugin-vuex": "^4.1.0-beta.0",
    "@vue/cli-service": "^4.1.0-beta.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "keyword": [
    "vue",
    "Forward refresh, back not refresh",
    "前进刷新，后退不刷新",
